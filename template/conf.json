{
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": ["plugins/markdown",
    "../foodoc/node_modules/jsdoc-mermaid"
  ],
  "templates": {
    "includeDate": true,
    "dateFormat": "Do MMM YYYY",
    "includeClass": true,
    "includeGeneratedBy": true,
    "removeInheritedStubs": false,
    "systemName": "FooDoc",
    "systemSummary": "A Bootstrap and Handlebars based JSDoc3 template.",
    "systemLogo": "",
    "pageLogo": "",
    "systemColor": "",
    "copyright": "FooDoc Copyright Â© 2016 The contributors to the JSDoc3 and FooDoc projects.",
    "linenums": true,
    "collapseSymbols": true,
    "inverseNav": true,
    "inlineNav": false,
    "outputSourceFiles": true,
    "outputSourcePath": false,
    "disablePackagePath": true,
    "methodHeadingReturns": true,
    "showTableOfContents": true,
    "showAccessFilter": false,
    "includeMermaid": false,
    "sort": "linenum, longname, version, since",
    "search": true,
    "analytics": {"ua": "", "domain": ""},
    "favicon": "",
    "stylesheets": [],
    "scripts": []
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  }
}
