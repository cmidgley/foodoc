{{#if classdesc}}
	{{#callout "Description" "callout-primary"}}
		<div class="description classdesc">{{{classdesc}}}</div>
	{{/callout}}
{{else if description}}
	{{#callout "Description" "callout-primary"}}
		<div class="description">{{{description}}}</div>
	{{/callout}}
{{/if}}
{{#if classDiagram}}
	{{#callout "Class Diagram" "callout-class-diagram"}}
		<div>{{{classDiagram}}}</div>
	{{/callout}}
{{/if}}
