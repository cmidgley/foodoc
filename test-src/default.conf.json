{
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": ["plugins/markdown"
  ],
  "opts": {
    "access": "all"
  },
  "templates": {
    "default": {
      "staticFiles": {
        "include": [
          { "filePath": "images",
            "outputPath": "images" }
        ]
      }
    },
    "useShortNamesInLinks": true,
    "includeDate": true,
    "dateFormat": "Do MMM YYYY",
    "includeClass": false,
    "includeGeneratedBy": false,
    "removeInheritedStubs": true,
    "removeInheritedPrivate": true,
    "systemName": "FooDoc",
    "systemSummary": "A Bootstrap and Handlebars based JSDoc3 template.",
    "systemLogo": "",
    "pageLogo": "",
    "systemColor": "",
    "copyright": "FooDoc Copyright Â© 2016 The contributors to the JSDoc3 and FooDoc projects.",
    "linenums": true,
    "collapseSymbols": true,
    "inverseNav": true,
    "inlineNav": false,
    "outputSourceFiles": true,
    "outputSourcePath": false,
    "methodHeadingReturns": true,
    "showTableOfContents": true,
    "expandTableOfContents": true,
    "showAccessFilter": true,
    "defaultFilterInherited": true,
    "defaultFilterPublic": true,
    "defaultFilterProtected": false,
    "defaultFilterPrivate": false,
    "includeMermaid": true,
    "includeClassDiagrams": true,
    "sort": "linenum, longname, version, since",
    "search": true,
    "analytics": {"ua": "", "domain": ""},
    "noRobots": false,
    "favicon": "",
    "stylesheets": [],
    "scripts": []
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": false
  }
}
